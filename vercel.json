{
  "version": 2,
  "buildCommand": "nx build locatr-backend",
  "builds": [
    {
      "src": "apps/locatr-backend/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "outputDirectory": "dist/apps/locatr-backend"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/locatr-backend/src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ]
}
